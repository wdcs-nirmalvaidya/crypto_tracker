{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "types": ["vite/client"],

    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,

    "jsx": "react-jsx",

    "strict": true,
    "skipLibCheck": true,

    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,

    "forceConsistentCasingInFileNames": true,
    "noEmit": true
  },
  "include": ["src"]
}
